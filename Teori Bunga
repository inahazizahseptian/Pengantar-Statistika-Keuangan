# Pengantar-Statistika-Keuangan
syntax mencari teori bunga yang digunakan pada Statistika Keuangan

bunga=function(num,At,k,t,m=TRUE){
  switch(num,
         satu={bunga=(At/k-1)/t
         },
         dua={bunga=m*((At/k)^(1/(m*t))-1)
         },
         tiga={bunga=log(At/k)/t
         })
return(bunga)}
